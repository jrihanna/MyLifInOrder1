    <LinearLayout
        android:id="@+id/mainContainer"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintTop_toTopOf="parent"
        android:padding="5dp"
        >

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:gravity="center_vertical">

            <ImageView
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:paddingEnd="5dp"
                android:src="@drawable/mushroomhouse"
                android:contentDescription="House icon" />
            <TextView
                android:id="@+id/residenceHistory"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/residence_history"
                android:clickable="true"
                android:focusable="true"
                android:minHeight="50dp"
                android:textSize="20sp"
                android:layout_marginTop="10dp"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:gravity="center_vertical">

            <ImageView
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:paddingEnd="5dp"
                android:src="@drawable/mushroomhouse"
                android:contentDescription="House icon" />
            <TextView
                android:id="@+id/employmentHistory"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/employment_history"
                android:clickable="true"
                android:focusable="true"
                android:minHeight="50dp"
                android:textSize="20sp"
                android:layout_marginTop="10dp"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:gravity="center_vertical">

            <ImageView
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:paddingEnd="5dp"
                android:src="@drawable/mushroomhouse"
                android:contentDescription="House icon" />
            <TextView
                android:id="@+id/educationHistory"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/education_history"
                android:clickable="true"
                android:focusable="true"
                android:minHeight="50dp"
                android:textSize="20sp"
                android:layout_marginTop="10dp"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:gravity="center_vertical">

            <ImageView
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:paddingEnd="5dp"
                android:src="@drawable/mushroomhouse"
                android:contentDescription="House icon" />
            <TextView
                android:id="@+id/subscription"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/subscriptions"
                android:clickable="true"
                android:focusable="true"
                android:minHeight="50dp"
                android:textSize="20sp"
                android:layout_marginTop="10dp"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:gravity="center_vertical">

            <ImageView
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:paddingEnd="5dp"
                android:src="@drawable/mushroomhouse"
                android:contentDescription="House icon" />
            <TextView
                android:id="@+id/insurances"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/insurances"
                android:clickable="true"
                android:focusable="true"
                android:minHeight="50dp"
                android:textSize="20sp"
                android:layout_marginTop="10dp"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:gravity="center_vertical">

            <ImageView
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:paddingEnd="5dp"
                android:src="@drawable/mushroomhouse"
                android:contentDescription="House icon" />
            <TextView
                android:id="@+id/loans"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/loans"
                android:clickable="true"
                android:focusable="true"
                android:minHeight="50dp"
                android:textSize="20sp"
                android:layout_marginTop="10dp"/>
        </LinearLayout>


    </LinearLayout>

